package corejava;

 class sum {
	 //sum(). This sum takes two int parameters
	 public int sum(int x,int y ) {
		 return(x+y);
		 
	 }
	 //sum(). This sum takes three int parameters
	 public int sum (int x, int y, int z) {
		 return(x+y+z);
		 
	 }
	 //sum(). This sum takes two double
	 public double sum(double x, double y) {
		 return(x+y);
		 
	 }
	  // Driver code
	 public static void main(String args[]) {
	 
		 sum s = new sum();
		 System.out.println(s.sum(10,20));
		 System.out.println(s.sum(20,40,60));
		 System.out.println(s.sum(13.5,25.6));
		 
		 
	 }
}
